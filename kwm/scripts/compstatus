echo "$(date +%l:%M%p)@$(date +%d%b%y)@$(pmset -g batt | egrep '([0-9]+\%).*' -o --colour=auto | cut -f1 -d'%')%@$(pmset -g batt | grep "'.*'" | sed "s/'//g" | cut -c 18-19)@$(/usr/local/bin/python3 ~/.kwm/scripts/weatherscript.py)@$(ps -axC -o %cpu | awk '{s+=$1} END {print s}')@$(ps -A -o %mem | awk '{s+=$1} END {print s}')@$(sh ~/.kwm/scripts/networktraffic)"
